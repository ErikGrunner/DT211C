#include <stdio.h>

int main()
{
    int age=0;
    int minutes=525960;
    int beats=75;
    int life = 0;
    
    scanf("%d",&age);
    flushall();
    
    life = age*minutes*beats;
    
    printf("Your heart beat %d times in your %d years",life,age);
    
    getchar();
    
    return 0;